<?xml version="1.0"?>
  <PropertyList> 
    <path>seafire-boost.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>boost-Needle1</object-name>
      <property>engines/engine/boost-gauge-psi</property>
   <!--    <factor>0.4911</factor>
     <offset>-29.92</offset> -->
	  
      <interpolation> 
	  <entry> 
          <ind>-20</ind>
          <dep>-85</dep>
        </entry>
        <entry> 
          <ind>-10</ind>
          <dep>-45</dep>
        </entry>
        <entry> 
          <ind>-5</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>0</ind>
          <dep>45</dep>
        </entry>
        <entry> 
          <ind>5</ind>
          <dep>90</dep>
        </entry>
        <entry> 
          <ind>10</ind>
          <dep>135</dep>
        </entry>
        <entry> 
          <ind>15</ind>
          <dep>180</dep>
        </entry>
        <entry> 
          <ind>20</ind>
          <dep>225</dep>
        </entry>
        <entry> 
          <ind>35</ind>
          <dep>265</dep>
        </entry>
      </interpolation>
	 
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
