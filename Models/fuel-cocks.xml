<?xml version="1.0"?>
  <PropertyList> 
    <path>fuel-cocks.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>lever-upper</object-name>
      <property>consumables/fuel/tank[0]/selected</property>
      <interpolation> 
        <entry> 
          <ind>0.0</ind>
          <dep>-45</dep>
        </entry>
        <entry> 
          <ind>1.0</ind>
          <dep>45</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>-0.02</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
	  <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>rotate</type>
      <object-name>lever-lower</object-name>
      <property>consumables/fuel/tank[1]/selected</property>
      <interpolation> 
        <entry> 
          <ind>0.0</ind>
          <dep>-45</dep>
        </entry>
        <entry> 
          <ind>1.0</ind>
          <dep>45</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>-0.02</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
	  <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
	
  </PropertyList>
