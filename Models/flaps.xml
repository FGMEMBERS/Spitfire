<?xml version="1.0"?>
  <PropertyList> 
    <path>flaps.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>Handle</object-name>
      <property>controls/flight/flaps-lever</property>
      <interpolation> 
        <entry> 
          <ind>0</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1</ind>
          <dep>90</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>0.008</x-m>
        <y-m>0</y-m>
        <z-m>0.008</z-m>
      </center>
      <axis> 
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </axis>
    </animation>
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
