<?xml version="1.0"?>
<!-- turn and slip indicator -->
<PropertyList>
	<path>triple.ac</path>
	<animation>
		<type>rotate</type>
		<object-name>Supply-Needle1</object-name>
		<property>systems/pneumatic/relief-valves/pressure-reducing/pressure-in-psi</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-90</dep>
			</entry>
			<entry>
				<ind>100</ind>
				<dep>-60</dep>
			</entry>
			<entry>
				<ind>200</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>300</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>400</ind>
				<dep>30</dep>
			</entry>
			<entry>
				<ind>500</ind>
				<dep>60</dep>
			</entry>
			<entry>
				<ind>600</ind>
				<dep>90</dep>
			</entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>L-Brake-Needle2</object-name>
		<property>systems/pneumatic/proportionators/brakes/pressure-out-psi</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-80</dep>
			</entry>
			<entry>
				<ind>100</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>200</ind>
				<dep>80</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>-0.004415</y-m>
			<z-m>-0.00798</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>R-Brake-Needle3</object-name>
		<property>systems/pneumatic/proportionators/brakes/pressure-out-psi[1]</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-80</dep>
			</entry>
			<entry>
				<ind>100</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>200</ind>
				<dep>80</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0.004415</y-m>
			<z-m>-0.00798</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>material</type>
		<condition>
			<greater-than>
				<property>/controls/lighting/panel-norm</property>
				<value>0</value>
			</greater-than>
			<not>
				<property>/sim/rendering/rembrandt/enabled</property>
			</not>
		</condition>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>
	
	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>
</PropertyList>

