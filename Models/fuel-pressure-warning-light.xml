<?xml version="1.0"?>
<PropertyList>
	<path>fuel-pressure-warning-light.ac</path>

	<animation>
		<type>material</type>
		<condition>
			<greater-than>
				<property>/controls/lighting/panel-norm</property>
				<value>0</value>
			</greater-than>
		</condition>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>

	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>
</PropertyList>
