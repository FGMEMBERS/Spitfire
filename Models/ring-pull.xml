<?xml version="1.0"?>
<PropertyList>
	<path>ring-pull.ac</path>

	<animation>
		<type>pick</type>
		<visible>false</visible>
		<object-name>box</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					spitfire.indexCof(1);
				</script>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
					<script>
						spitfire.indexCof(0);
					</script>
				</binding>
			</mod-up>
		</action>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>ring</object-name>
		<property>controls/engines/engine/coffman-starter/index-pull-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>90</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.0211067</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>ring-pull</object-name>
		<property>controls/engines/engine/coffman-starter/index-pull-norm</property>
		<factor>0.035</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>material</type>
		<condition>
			<greater-than>
				<property>/controls/lighting/panel-norm</property>
				<value>0</value>
			</greater-than>
		</condition>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>

	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>
</PropertyList>
